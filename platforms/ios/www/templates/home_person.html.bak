    <div style="height: 90px;background-color: white;margin-top:10px;border-top:1px solid #f0f0f0;border-bottom: 1px solid #f0f0f0" ng-click="personnal_data_path()">
      <div style="width: 65px;height: 65px;overflow: hidden;border-radius: 150px;margin-top: 12px;float: left;margin-left: 12px;">
        <img ng-src="{{personnal_data.img_src}}" alt="" style="width: 65px;height: 65px;"/>
      </div>
      <div style="float:right;">
        <i class="icon ion-ios-arrow-right" style="margin-top:10px; margin-right:12px; line-height:89px;vertical-align:middle;font-size:20px;color:#ccc; margin-left:9px"></i>
      </div>
      <div style="font-size: 18px;color: #333;text-indent: 10px;margin-top: 16px;">{{personnal_data.employee_name}}</div>
      <div style="font-size: 14px;color: #999;text-indent: 10px;margin-top: 18px;">工号:{{personnal_data.staff_no}}</div>
    </div>

