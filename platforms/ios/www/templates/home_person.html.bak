    <div style="height: 44px;background-color: white;border-bottom: 1px solid #f0f0f0;border-top: 1px solid #f0f0f0;margin-top: 10px;"  ng-click="my_business()">
      <img src="{{my_business_icon}}" alt="" style="width: 22px;height: 22px;float: left;margin-left: 12px;margin-top: 11px;"/>
      <div style="float:right;">
        <i class="icon ion-ios-arrow-right" style="margin-top:10px; margin-right:12px; line-height:43px;vertical-align:middle;font-size:20px;color:#ccc; margin-left:9px"></i>
      </div>
      <div style="font-size: 16px;color: #333;text-indent: 12px;line-height: 44px;">我的企业</div>
    </div>

    <div style="height: 44px;background-color: white;border-top: 1px solid #f0f0f0;margin-top: 10px;position: relative;overflow: hidden" ng-click="change_username()">
      <img src="{{change_user_icon}}" alt="" style="width: 22px;height: 22px;float: left;margin-left: 12px;margin-top: 11px;"/>
      <div style="float:right;">
        <i class="icon ion-ios-arrow-right" style="margin-top:10px; margin-right:12px; line-height:43px;vertical-align:middle;font-size:20px;color:#ccc; margin-left:9px"></i>
      </div>
      <div style="font-size: 16px;color: #333;text-indent: 12px;line-height: 44px;">更改用户名</div>
      <div style="width: 100%;height: 1px;border-top: 1px solid #f0f0f0;position: absolute;left: 12px;bottom: 0px;"></div>
    </div>
    <div style="height: 44px;background-color: white;margin-top: 0px;border-bottom: 1px solid #f0f0f0" ng-click="resign_user()">
      <img src="{{cancellation}}" alt="" style="width: 22px;height: 22px;float: left;margin-left: 12px;margin-top: 11px;"/>
      <div style="float:right;">
        <i class="icon ion-ios-arrow-right" style="margin-top:10px; margin-right:12px; line-height:43px;vertical-align:middle;font-size:20px;color:#ccc; margin-left:9px"></i>
